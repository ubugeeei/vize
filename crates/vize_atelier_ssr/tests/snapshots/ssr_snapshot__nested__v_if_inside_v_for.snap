function ssrRender(_ctx, _push, _parent, _attrs) {
  _push(`<!--[-->`)
  _ssrRenderList(_ctx.items, (item) => {
    _push(`<div>`)
    if (item.ok) {
      _push(`<span>${_ssrInterpolate(item.text)}</span>`)
    } else {
      _push(`<!---->`)
    }
    _push(`</div>`)
  })
  _push(`<!--]-->`)
}
