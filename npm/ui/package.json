{
  "name": "@vizejs/ui",
  "version": "0.0.1-alpha.72",
  "description": "Headless, type-safe, accessible Vue UI primitives",
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "musea": "vite dev",
    "lint": "oxlint --deny-warnings --type-aware --tsconfig tsconfig.json",
    "lint:fix": "oxlint --type-aware --tsconfig tsconfig.json --fix",
    "fmt": "oxfmt --write src",
    "fmt:check": "oxfmt src"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "ui",
    "headless",
    "accessible",
    "a11y",
    "primitives",
    "components",
    "vize"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ubugeeei/vize",
    "directory": "npm/ui"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vizejs/vite-plugin": "workspace:*",
    "@vizejs/vite-plugin-musea": "workspace:*",
    "vite": "^8.0.0-beta.0",
    "tsdown": "^0.9.0",
    "typescript": "^5.7.0",
    "vue": "^3.5.0"
  }
}
